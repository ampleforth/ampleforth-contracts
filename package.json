{
  "name": "uFragments",
  "version": "0.0.1",
  "description": "Î¼Fragments protocol smart contracts on Ethereum.",
  "keywords": [
    "etheruem",
    "smart-contracts",
    "solidity"
  ],
  "homepage": "https://github.com/frgprotocol/uFragments#readme",
  "bugs": {
    "url": "https://github.com/frgprotocol/uFragments/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frgprotocol/uFragments.git"
  },
  "license": "ISC",
  "author": "eng@fragments.org",
  "scripts": {
    "blockchain:start": "./scripts/frg-ethereum-runners/exec.sh start",
    "blockchain:stop": "./scripts/frg-ethereum-runners/exec.sh stop",
    "precommit": "./scripts/pre-commit.sh",
    "deploy-contracts": "truffle migrate --reset --network",
    "genDummyData": "truffle --network gethUnitTest exec ./scripts/gen_dummy_data.js",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:js": "node_modules/eslint/bin/eslint.js ./ --cache --fix",
    "lint:sol": "node_modules/solhint/solhint.js contracts/*.sol",
    "test": "./scripts/test.sh",
    "trackGasUtilization": "npm run blockchain:start ganacheUnitTest && npm run deploy-contracts ganacheUnitTest && npm run truffle ganacheUnitTest exec test/load/gas_utilization.js save",
    "truffle": "truffle --network"
  },
  "pre-commit": {
    "run": [
      "precommit",
      "lint:js",
      "lint:sol",
      "trackGasUtilization"
    ]
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.5",
    "openzeppelin-solidity": "1.10.0",
    "web3": "github:ethereum/web3.js"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-bignumber": "^2.0.2",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-mocha": "0.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-mocha-only": "0.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-spellcheck": "0.0.10",
    "eslint-plugin-standard": "^1.3.3",
    "ethereumjs-abi": "^0.6.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "sinon": "^4.5.0",
    "solhint": "^1.1.10",
    "stochasm": "^0.5.0",
    "truffle-hdwallet-provider": "0.0.5",
    "webpack": "^4.10.2"
  }
}
